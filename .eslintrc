{
    "env": {
        "es6": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": ["import", "prettier"],
    "root": true,
    "rules": {
        "global-require": 0,
        "indent": ["error", 4],
        "strict": ["error", "global"]
    }
}
